{
  "name": "@genomeforge/api",
  "version": "0.1.0",
  "description": "GenomeForge API SDK for third-party integrations",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@genomeforge/core": "workspace:*",
    "@genomeforge/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "genomeforge",
    "genetic",
    "genomics",
    "api",
    "sdk",
    "dna"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmhernandez2525/genomeforge.git",
    "directory": "packages/api"
  }
}
